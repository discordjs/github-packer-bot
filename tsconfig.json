{
	"compilerOptions": {
		"strict": true,
		"alwaysStrict": true,
		"moduleResolution": "node16",
		"removeComments": true,
		"pretty": true,
		"module": "Node16",
		"target": "ES2020",
		"lib": ["ESNext"],
		"sourceMap": true,
		"incremental": false,
		"skipLibCheck": true,
		"noEmitHelpers": true,
		"importHelpers": true,
		"esModuleInterop": true,
		"noUncheckedIndexedAccess": true,
		"noImplicitOverride": true,
		"verbatimModuleSyntax": true,
		"declaration": false,
		"noEmit": true,
		"types": ["@cloudflare/workers-types"]
	},
	"include": ["./src/**/*.ts"]
}
